{
  "name": "lexical-outline",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm --filter=@meogic/* run build",
    "clear": "rimraf 'packages/*/{lib,node_modules}' && rimraf node_modules",
    "changeset": "changeset",
